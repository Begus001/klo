{
    "manifest_version": 2,
    "name": "Klo",
    "description": "WC - WatchCollectively (or Klo)",
    "version": "1",
    "permissions": [
        "webRequest",
        "activeTab",
        "tabs",
        "webNavigation",
        "scripting"
    ],
    "content_scripts": [
        {
            "matches": [
                "*://*/*"
            ],
            "js": [
                "content-script.js"
            ],
            "run_at": "document_end"
        }
    ]
}
